{
  "name": "agari-kai",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --watch",
    "e2e": "ng run agari-kai:cypress-run",
    "check-all": "prettier . -c",
    "format-all": "prettier . -w",
    "lint": "ng lint",
    "serve:pwa": "http-server -p 8080 -c-1 dist/agari-kai",
    "ci:build": "ng build --configuration=development",
    "ci:unit": "jest --runInBand",
    "cd:build": "ng build --configuration=production"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "private": true,
  "engines": {
    "node": "^20.9.0"
  },
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/service-worker": "^17.0.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "~17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@cypress/schematic": "^2.5.1",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cypress": "^13.6.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.4",
    "prettier": "^3.1.1",
    "typescript": "~5.2.2"
  }
}
